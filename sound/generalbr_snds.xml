<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="GeneralBr">
		<soundtype name="Select">
			<soundset name="BritishHeavyCavalrySelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MilitaryBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="BritishHeavyCavalryAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="BritishHeavyCavalryAttack"/>
				</choice>
				<choice name="build">
					<soundset name="BritishHeavyCavalryAcknowledge"/>
				</choice>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
				<choice name="AbstractResourceCrate"/>
				<choice name="AbstractFruit"/>
			</targetlogic>
		</soundtype>
		<soundtype name="KnockOut">
			<soundset name="BritishExplorerDisabled"/>
		</soundtype>
		<soundtype name="KnockOutRevived">
			<soundset name="BritishExplorerRevived"/>
		</soundtype>
		<soundtype name="Ransomed">
			<soundset name="BritishExplorerRansomed"/>
		</soundtype>
	</protounit>
</protounitsounddef>