<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="ypMonkChinese">
		<soundtype name="Select">
			<soundset name="ChineseMonkSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="ChineseMonkRevived"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="ChineseMonkAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="ChineseMonkAttack"/>
				</choice>
				<choice name="build">
					<soundset name="ChineseMonkAcknowledge"/>
				</choice>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
				<choice name="AbstractResourceCrate"/>
				<choice name="AbstractFruit"/>
			</targetlogic>
		</soundtype>
		<soundtype name="Claim">
			<soundset name="ChineseMonkClaim"/>
		</soundtype>
		<soundtype name="KnockOut">
			<soundset name="ChineseMonkDisabled"/>
		</soundtype>
		<soundtype name="KnockOutRevived">
			<soundset name="ChineseMonkRevived"/>
		</soundtype>
	</protounit>
</protounitsounddef>