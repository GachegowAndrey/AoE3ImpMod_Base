<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="xpLakotaWarchief">
		<soundtype name="Select">
			<soundset name="SiouxWarChiefSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="CavalryGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="SiouxWarChiefSelect"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="SiouxWarChiefAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="SiouxWarChiefAttack"/>
				</choice>
				<choice name="build">
					<soundset name="SiouxWarChiefAcknowledge"/>
				</choice>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
				<choice name="AbstractResourceCrate"/>
				<choice name="AbstractFruit"/>
			</targetlogic>
		</soundtype>
		<soundtype name="KnockOutRevived">
			<soundset name="SiouxWarChiefSelect"/>
		</soundtype>
		<soundtype name="Ransomed">
			<soundset name="SiouxWarChiefSelect"/>
		</soundtype>
	</protounit>
</protounitsounddef>