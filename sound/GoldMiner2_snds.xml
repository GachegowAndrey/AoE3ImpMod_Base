<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="GoldMiner">
		<soundtype name="Select">
			<soundset name="SPCMinerSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="SettlerBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="SPCMinerAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="SPCMinerAttack"/>
				</choice>
				<choice name="build">
					<soundset name="SPCMinerAcknowledge"/>
				</choice>
				<choice name="AbstractMine">
					<soundset name="SPCMinerAcknowledge"/>
				</choice>
				<choice name="AbstractResourceCrate">
					<soundset name="SPCMinerAcknowledge"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>