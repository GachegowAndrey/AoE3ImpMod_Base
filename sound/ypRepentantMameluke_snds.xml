<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="ypRepentantMameluke">
		<soundtype name="Grunt">
			<soundset name="CamelGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Select">
			<soundset name="EgyptianMamelukeSelect"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="EgyptianMamelukeAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="EgyptianMamelukeAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MilitaryBirth"/>
		</soundtype>
	</protounit>
</protounitsounddef>