	<submodel>
		wood
		<attachment>
			base smoke
			<component>
				smoke base
				<assetreference type="ParticleSystem">
					<file>effects\smoke\collapsesmoke01.particle</file>
				</assetreference>
			</component>
			<anim>
				Idle
				<component>smoke base</component>
			</anim>
		</attachment>
		<attachment>
			collapse_smoke
			<component>
				collapse_smoke
				<assetreference type="ParticleSystem">
					<file>effects\smoke\collapse_smoke_a.particle</file>
				</assetreference>
			</component>
			<anim>
				Idle
				<component>collapse_smoke</component>
			</anim>
		</attachment>
		<component>
			ModelComp
			<assetreference type="GrannyModel">
				<file>buildings\wall\wall_tower_u1</file>
			</assetreference>
			<decal>
				<effecttype>bump</effecttype>
				<texture>buildings\wall\wall_wood_stone_ground_small</texture>
				<selectedtexture>shadows_selections\selection_square_128x128</selectedtexture>
				<bumptexture>shadows_selections\flat_normal_black_spec</bumptexture>
				<width>3.50</width>
				<height>3.50</height>
				<xoffset>0.00</xoffset>
				<zoffset>0.00</zoffset>
			</decal>
		</component>
		<anim>
			Idle
			<component>ModelComp</component>
			<simskeleton>
				<model>buildings\wall\wall_wooden_corner_damaged</model>
				<damagetemplate>buildings\wall\wall_wooden_corner.dmg</damagetemplate>
			</simskeleton>
		</anim>
	<anim>
		RangedAttack
		<assetreference type="GrannyAnim">
			<file>animation_library\building\ranged_attack</file>
			<tag type="Attack">0.98</tag>
			<tag type="SpecificSoundSet" checkvisible="1" set="Arrows">0.98</tag>
		</assetreference>
		<component>ModelComp</component>
		<simskeleton>
				<model>buildings\wall\wall_wooden_corner_damaged</model>
				<damagetemplate>buildings\wall\wall_wooden_corner.dmg</damagetemplate>
		</simskeleton>
	</anim>
	<anim>
