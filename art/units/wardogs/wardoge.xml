<?xml version="1.0" encoding="utf-8"?>

<animfile>
	<definebone>Bone_rt_foot</definebone>
	<definebone>Bone_lt_foot</definebone>
	<component>
		ModelComp
		<logic type="Variation">
			<data>
				<logic type="LowPoly">
					<normal>
						<assetreference type="GrannyModel">
							<file>units\wardogs\wardog1</file>
						</assetreference>
					</normal>
					<lowpoly>
						<assetreference type="GrannyModel">
							<file>units\wardogs\lp_wardog1</file>
						</assetreference>
					</lowpoly>
				</logic>
				<decal>
					<effecttype>default</effecttype>
					<texture isfakeshadow="1">shadows_selections\shadow_circle_64x64</texture>
					<selectedtexture>shadows_selections\selection_hero_64x64</selectedtexture>
					<noncinematictexture>shadows_selections\shadow_hero_64x64</noncinematictexture>
					<width>1.50</width>
					<height>1.50</height>
				</decal>
			</data>
			<data>
				<logic type="LowPoly">
					<normal>
						<assetreference type="GrannyModel">
							<file>units\wardogs\wardog2</file>
						</assetreference>
					</normal>
					<lowpoly>
						<assetreference type="GrannyModel">
							<file>units\wardogs\lp_wardog2</file>
						</assetreference>
					</lowpoly>
				</logic>
				<decal>
					<effecttype>default</effecttype>
					<texture isfakeshadow="1">shadows_selections\shadow_circle_64x64</texture>
					<selectedtexture>shadows_selections\selection_hero_64x64</selectedtexture>
					<noncinematictexture>shadows_selections\shadow_hero_64x64</noncinematictexture>
					<width>1.50</width>
					<height>1.50</height>
				</decal>
			</data>
		</logic>
	</component>
	<anim>
		Idle
		<assetreference type="GrannyAnim">
			<file>animation_library\quads\wolf_idlea</file>
		</assetreference>
		<component>ModelComp</component>
	</anim>
	<anim>
		Bored
		<assetreference type="GrannyAnim">
			<file>animation_library\quads\wolf_boreda</file>
		</assetreference>
		<assetreference type="GrannyAnim">
			<file>animation_library\quads\wolf_boredb</file>
		</assetreference>
		<component>ModelComp</component>
	</anim>
	<anim>
		Walk
		<assetreference type="GrannyAnim">
			<file>animation_library\quads\wolf_walka</file>
			<tag type="FootstepLeft" footprinttype="AnimalLeft" footprintbone="Bone_lt_foot">0.93</tag>
			<tag type="FootstepRight" footprinttype="AnimalRight" footprintbone="Bone_rt_foot">0.13</tag>
		</assetreference>
		<component>ModelComp</component>
	</anim>
	<anim>
		HandAttack
		<assetreference type="GrannyAnim">
			<file>animation_library\quads\wolf_attacka</file>
			<tag type="Attack">0.67</tag>
		</assetreference>
		<component>ModelComp</component>
	</anim>
	<anim>
		Death
		<assetreference type="GrannyAnim">
			<file>animation_library\quads\wolf_deatha</file>
			<tag type="SpecificSoundSet" checkvisible="1" set="RagdollImpact">0.80</tag>
		</assetreference>
		<component>ModelComp</component>
	</anim>
</animfile>